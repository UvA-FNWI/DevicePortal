{
  "compileOnSave": true,
  "compilerOptions": {
    "outDir": "wwwroot/js",
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": false,
    "inlineSources": true,
    "inlineSourceMap": true,
    "target": "es5",
	"typeRoots": [ "./Scripts/typings" ]
  },
  "include": [ "./Scripts/**/*" ],
  "exclude": [
    "node_modules",
    "wwwroot"
  ]
}
